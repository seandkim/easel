{
  "weekdays": 
    [
        {{mon}},
        {{tue}},
        {{wed}},
        {{thu}},
        {{fri}},
        {{sat}},
        {{sun}}
    ],
  "sites":
    [
      {% for site in sites %}
         {
           "name": "{{site.name}}"
         }{% if not forloop.last %},{% endif %}
      {% endfor %}
    ]
}

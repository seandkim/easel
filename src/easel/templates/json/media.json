{
  "media":
    [
      {% for medium in media %}
         {
           "id": "{{medium.id}}",
           "name": "{{medium.name}}",
           "type": "{{medium.type}}",
           "fileURL": "{{medium.fileURL}}",
           "caption": "{{medium.caption}}"
         }{% if not forloop.last %},{% endif %}
      {% endfor %}
    ]
}
